@page
@model Event_application.Pages.VIP.VIPModel
@{
}

<h1>Almindelig menu </h1>

<h6> Herunder kan du set et overblik over vores almindelige menu</h6>
<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Navn
            </th>
            <th>
                Beskrivelse
            </th>
            <th>
                Pris
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.al)
        {
            <tr>
                <td>
                    @item.Value.Id
                </td>
                <td>
                    @item.Value.Navn
                </td>
                <td>
                    @item.Value.Beskrivelse
                </td>
                <td>
                    @item.Value.Pris
                </td>

                <td>
                    <img src="~/@item.Value.Billede" width="100" height="110">
                </td>
            </tr>
        }
    </tbody>
</table>

