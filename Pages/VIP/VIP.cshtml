@page
@model Event_application.Pages.VIP.VIPModel
@{
}

<h1>Almindelig menu </h1>


<table class="table">
    <thead>
    <tr>
        <th>
            Id
        </th>
        <th>
            Navn
        </th>
        <th>
            Beskrivelse
        </th>
        <th>
            Pris
        </th>
                
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.al)
    {
        <tr>
            <td>
                @item.Value.Id
            </td>
            <td>
                @item.Value.Navn
            </td>
            <td>
                @item.Value.Beskrivelse
            </td>
            <td>
                @item.Value.Billede
            </td>
            <td>
                <img src="~/@item.Value.Billede"  width="60" height="70">
            </td>
        </tr>
    }
    </tbody>
</table>



